package exercicios;

public class TestePessoa {

	public static void main(String[] args) {
			Pessoa pessoa1 = new Pessoa();
			Pessoa pessoa2 = new Pessoa();
			
			pessoa1.nome = "Mancel";
			pessoa1.peso = 67.8;
			pessoa1.altura = 1.72;
			
			pessoa2.nome = "Ana";
			pessoa2.peso = 77.8;
			pessoa2.altura = 1.82;
			
			
			System.out.println(pessoa1.nome + " voce está " + pessoa1.resultado());
			System.out.println(pessoa2.nome + " voce está " + pessoa2.resultado());
	}

}
